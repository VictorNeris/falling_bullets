rem *** Put non level specific initializations here ***


rem *** Load multiplayer free camera here
run Common/FreeCamera/Object.con

gameLogic.setDefaultNumberOfTickets 1 250
gameLogic.setDefaultNumberOfTickets 2 250

gameLogic.enableVoHelp 0
gameLogic.supplyDropHeight 50
gameLogic.supplyDropNumSecsToLive 300

rem renderer.drawfps 1
renderer.drawnametags 1
sv.numPlayersNeededToStart 0
sv_spawntime 3

sv.radioSpamInterval 9999
sv.radioMaxSpamFlagCount 9999
sv.radioBlockedDurationTime 0
sv.friendlyFireWithMines 0

renderer.glowEnabled 1
renderer.glowStrength 1

rem renderer.fakeHDREnabled 1
rem renderer.fakeHDRPassGate 0.25
rem renderer.fakeHDRweights 0,0.025,0.05,0.075,0.1,0.075,0.05,0.025,0

renderer.enablehwocclusion 0

gameLogic.supplyDropHeight 200
